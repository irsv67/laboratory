<div class="particulars">
    <div class="dataSource">
        <span>数据源详情</span>
        <div class="amend">
            <cm-button>
                <button nz-button [nzType]="'primary'" [nzSize]="'large'" [routerLink]="['/main/<%=foo%>/modify',<%=foo%>Deta.id]" [disabled]="<%=foo%>Deta.status==0?true:false">
                    <span>修改</span>
                </button>
            </cm-button>
        </div>
    </div>
    <cm-form>
        <form nz-form [formGroup]="validateForm">
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSpan]="2">
                    <label>ID</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="20">
                    <span nz-form-text>{{<%=foo%>Deta.id}}</span>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSpan]="2">
                    <label>数据源名称</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="20">
                    <span nz-form-text [title]="<%=foo%>Deta.name">{{<%=foo%>Deta.name}}</span>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSpan]="2">
                    <label>数据源类型</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="20">
                    <span nz-form-text>{{<%=foo%>Deta.type}}</span>
                </div>
            </div>
            <div *ngIf="<%=foo%>Deta.type=='JDBC'">
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>主机IP</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>{{<%=foo%>Deta.host}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>数据库名称</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.database">{{<%=foo%>Deta.database}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>端口号</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>{{<%=foo%>Deta.port}}</span>

                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>用户名</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.userName">{{<%=foo%>Deta.userName}}</span>

                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>密码</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>*********</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>描述</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.description">{{<%=foo%>Deta.description}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>状态</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span *ngIf="<%=foo%>Deta.status!=0?true:false">正常</span>
                        <span *ngIf="<%=foo%>Deta.status==0?true:false">禁用</span>
                    </div>
                </div>
            </div>
            <div *ngIf="<%=foo%>Deta.type=='RESTAPI'">
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>URL</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>{{<%=foo%>Deta.url}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>描述</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.description">{{<%=foo%>Deta.description}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>状态</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span *ngIf="<%=foo%>Deta.status!=0?true:false">正常</span>
                        <span *ngIf="<%=foo%>Deta.status==0?true:false">禁用</span>
                    </div>
                </div>
            </div>
            <div *ngIf="<%=foo%>Deta.type=='ELASTICSEARCH'">
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>版本号</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>{{<%=foo%>Deta.version}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>URL</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>{{<%=foo%>Deta.url}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>描述</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.description">{{<%=foo%>Deta.description}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>状态</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span *ngIf="<%=foo%>Deta.status!=0?true:false">正常</span>
                        <span *ngIf="<%=foo%>Deta.status==0?true:false">禁用</span>
                    </div>
                </div>
            </div>
            <div *ngIf="<%=foo%>Deta.type=='SPARK'">
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>主机IP</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>{{<%=foo%>Deta.host}}</span>

                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>数据库名称</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.database">{{<%=foo%>Deta.database}}</span>

                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>spark库</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.database">{{<%=foo%>Deta.database}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>端口号</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>{{<%=foo%>Deta.port}}</span>

                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>用户名</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.userName">{{<%=foo%>Deta.userName}}</span>

                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>密码</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text>*********</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>描述</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span nz-form-text [title]="<%=foo%>Deta.description">{{<%=foo%>Deta.description}}</span>
                    </div>
                </div>
                <div nz-form-item nz-row>
                    <div nz-form-label nz-col [nzSpan]="2">
                        <label>状态</label>
                    </div>
                    <div nz-form-control nz-col [nzSpan]="20">
                        <span *ngIf="<%=foo%>Deta.status!=0?true:false">正常</span>
                        <span *ngIf="<%=foo%>Deta.status==0?true:false">禁用</span>
                    </div>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSpan]="2">
                    <label>创建人</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="20">
                    <span>{{<%=foo%>Deta.creator}}</span>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSpan]="2">
                    <label>创建时间</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="20">
                    <span>{{<%=foo%>Deta.createTime}}</span>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSpan]="2">
                    <label>更新人</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="20">
                    <span>{{<%=foo%>Deta.updater}}</span>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSpan]="2">
                    <label>更新时间</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="20">
                    <span>{{<%=foo%>Deta.updateTime}}</span>
                </div>
            </div>
        </form>
    </cm-form>
</div>
