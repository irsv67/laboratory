.ant-form-item{
    margin-bottom:0px;
}
.ant-col-8{
    padding:0 !important;
        }
::ng-deep {
    .flexsearch{
        display: flex;
        width: 100%;
        >.source{
            flex: 9;
            padding-right:24px;
           
            >.ant-form-item-label{                     
                >label{
                font-family: PingFangSC-Medium;
                font-size: 12px;
                color: #17233D;
                text-align: left !important;
                width: 72px !important;
                display: inline-block;
                }
            }
            .ant-col-7{
                width: 80px !important;
                line-height: 32px;
            }
        }
        >.lookUp{
            flex: 1;
            height: 32px;
             border-left: 1px solid rgba(23,35,61,0.10);
            padding-left: 24px;
        } 
    }  
