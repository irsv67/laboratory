::ng-deep {
    .ant-table-bordered .ant-table-thead > tr > th, .ant-table-bordered .ant-table-tbody > tr > td{
        border-right: none !important;
        }
    .<%=foo%>list{
       .list{
            display: inline-block;
            white-space:nowrap;
            overflow: hidden;
            text-overflow:ellipsis !important;
        }
        .ant-table-body{
            overflow-x: auto;
        }
    }

}