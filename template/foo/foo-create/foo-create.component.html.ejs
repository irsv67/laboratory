<div class="datesourceCreate">
    <div class="dataSource"><span><%=fooDesc%>信息</span></div>
    <cm-form *ngIf="singledom == 'JDBC'">
        <form nz-form style="width:70%" [formGroup]="validateForm">
            <div nz-form-item nz-row class="form-group">
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24">
                    <label for="'<%=foo%>Name'" nz-form-item-required><%=fooDesc%>名称</label>
                </div>
                <div nz-form-control nz-col [nzSm]="14" [nzXs]="24" nzHasFeedback>
                    <cm-input>
                        <nz-input [nzPlaceHolder]="'请输入<%=fooDesc%>名称'" [(ngModel)]="<%=foo%>Name" [formControlName]="'<%=foo%>Name'" [nzId]="'<%=foo%>Name'" [nzSize]="'large'"></nz-input>
                    </cm-input>
                    <div nz-form-explain *ngIf="getFormControl('<%=foo%>Name').dirty&&getFormControl('<%=foo%>Name').hasError('required')">{{regular.name.errorInfo.emptyInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('<%=foo%>Name').dirty&&getFormControl('<%=foo%>Name').hasError('duplicated')">{{regular.name.errorInfo.errorInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('<%=foo%>Name').dirty&&getFormControl('<%=foo%>Name').hasError('lengthInfo')">{{regular.name.errorInfo.lengthInfo}}</div>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSpan]="6">
                    <label nz-form-item-required for="'<%=foo%>Type'"><%=fooDesc%>类型</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="14">
                    <cm-select>
                        <nz-select [formControlName]="'<%=foo%>Type'" [nzSize]="'large'" [nzPlaceHolder]="'请输入<%=fooDesc%>类型'" [(ngModel)]="single" (ngModelChange)="selectedChange(single)" [(nzDisabled)]="disabled">
                            <nz-option *ngFor="let select of <%=foo%>TypeList" [nzLabel]="select.name" [nzValue]="select.value"></nz-option>
                        </nz-select>
                    </cm-select>
                    <div nz-form-explain *ngIf="getFormControl('<%=foo%>Type').dirty&&getFormControl('<%=foo%>Type').hasError('required')">请输入!</div>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24">
                    <label for="'dataIP'" nz-form-item-required>主机IP</label>
                </div>
                <div nz-form-control nz-col [nzSm]="14" [nzXs]="24" nzHasFeedback>
                    <cm-input>
                        <nz-input [formControlName]="'dataIP'" [nzId]="'dataIP'" [nzPlaceHolder]="'请输入主机IP'" [nzSize]="'large'"></nz-input>
                    </cm-input>
                    <div nz-form-explain *ngIf="getFormControl('dataIP').dirty&&getFormControl('dataIP').hasError('required')">{{regular.ip.errorInfo.emptyInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('dataIP').dirty&&getFormControl('dataIP').hasError('duplicated')">{{regular.ip.errorInfo.errorInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('dataIP').dirty&&getFormControl('dataIP').hasError('lengthInfo')">{{regular.ip.errorInfo.lengthInfo}}</div>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24">
                    <label for="'databaseName'" nz-form-item-required>数据库名称</label>
                </div>
                <div nz-form-control nz-col [nzSm]="14" [nzXs]="24" nzHasFeedback>
                    <cm-input>
                        <nz-input [formControlName]="'databaseName'" [nzId]="'databaseName'" [nzPlaceHolder]="'请输入数据库名称'" [nzSize]="'large'"></nz-input>
                    </cm-input>
                    <div nz-form-explain *ngIf="getFormControl('databaseName').dirty&&getFormControl('databaseName').hasError('required')">{{regular.name.errorInfo.emptyInfobaseName}}</div>
                    <div nz-form-explain *ngIf="getFormControl('databaseName').dirty&&getFormControl('databaseName').hasError('duplicated')">{{regular.name.errorInfo.errorInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('databaseName').dirty&&getFormControl('databaseName').hasError('lengthInfo')">{{regular.name.errorInfo.lengthInfo}}</div>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24">
                    <label for="port" nz-form-item-required>端口号</label>
                </div>
                <div nz-form-control nz-col [nzSm]="14" [nzXs]="24" nzHasFeedback>
                    <cm-input>
                        <nz-input [formControlName]="'port'" [nzId]="'port'" [nzPlaceHolder]="'请输入端口号'" [nzSize]="'large'"></nz-input>
                    </cm-input>
                    <div nz-form-explain *ngIf="getFormControl('port').dirty&&getFormControl('port').hasError('required')">{{regular.port.errorInfo.emptyInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('port').dirty&&getFormControl('port').hasError('duplicated')">{{regular.port.errorInfo.errorInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('port').dirty&&getFormControl('port').hasError('lengthInfo')">{{regular.port.errorInfo.lengthInfo}}</div>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24">
                    <label for="userName" nz-form-item-required>用户名</label>
                </div>
                <div nz-form-control nz-col [nzSm]="14" [nzXs]="24" nzHasFeedback>
                    <cm-input>
                        <nz-input [formControlName]="'userName'" [nzId]="'userName'" [nzPlaceHolder]="'请输入用户名'" [nzSize]="'large'"></nz-input>
                    </cm-input>
                    <div nz-form-explain *ngIf="getFormControl('userName').dirty&&getFormControl('userName').hasError('required')">{{regular.name.errorInfo.emptyInfoname}}</div>
                    <div nz-form-explain *ngIf="getFormControl('userName').dirty&&getFormControl('userName').hasError('duplicated')">{{regular.name.errorInfo.errorInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('userName').dirty&&getFormControl('userName').hasError('lengthInfo')">{{regular.name.errorInfo.lengthInfo}}</div>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24">
                    <label for="password" nz-form-item-required>密码</label>
                </div>
                <div nz-form-control nz-col [nzSm]="14" [nzXs]="24" nzHasFeedback>
                    <cm-input>
                        <nz-input [nzType]="'password'" [formControlName]="'password'" [nzId]="'password'" [nzPlaceHolder]="'请输入密码'" [nzSize]="'large'"></nz-input>
                    </cm-input>
                    <div nz-form-explain *ngIf="getFormControl('password').dirty&&getFormControl('password').hasError('required')">{{regular.password.errorInfo.emptyInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('password').dirty&&getFormControl('password').hasError('duplicated')">{{regular.password.errorInfo.errorInfo}}</div>
                    <div nz-form-explain *ngIf="getFormControl('password').dirty&&getFormControl('password').hasError('lengthInfo')">{{regular.password.errorInfo.lengthInfo}}</div>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24">
                    <label for="state">&nbsp;&nbsp;&nbsp;状态</label>
                </div>
                <div nz-form-control nz-col [nzSpan]="14">
                    <cm-switch>
                        <nz-switch [formControlName]="'state'" [(ngModel)]="state">
                            <span checked>正常</span>
                            <span unchecked>禁用</span>
                        </nz-switch>
                    </cm-switch>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24">
                    <label for="'<%=foo%>Matter'">&nbsp;&nbsp;&nbsp;描述</label>
                </div>
                <div nz-form-control nz-col [nzSm]="14" [nzXs]="24">
                    <cm-input>
                        <nz-input [nzType]="'textarea'" [nzSize]="'large'" [nzRows]="'4'" [formControlName]="'<%=foo%>Matter'" [nzId]="'<%=foo%>Matter'" style="width: 99.5%"></nz-input>
                    </cm-input>
                </div>
            </div>
            <div nz-form-item nz-row>
                <div nz-form-label nz-col [nzSm]="6" [nzXs]="24"></div>
                <div nz-form-control nz-col [nzSm]="14" [nzXs]="24">

                    <cm-button>
                        <button nz-button [nzType]="'primary'" [disabled]="!validateForm.valid" (click)="saveTheDataSource()" [nzSize]="'large'">确定</button>
                    </cm-button>
                    <div class="judge">
                        <cm-button>
                            <button nz-button [nzType]="'default'" (click)="backtrack ()" [nzSize]="'large'">取消</button>
                        </cm-button>
                    </div>
                    <cm-button>
                        <button nz-button [nzType]="'default'" (click)="testTheConnection()" [disabled]="!validateForm.valid" [nzSize]="'large'">测试连接</button>
                    </cm-button>
                </div>
            </div>

        </form>
    </cm-form>

